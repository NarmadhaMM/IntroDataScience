m3 <- matrix(0, ncol = c, nrow = r)
i <- 1
if(type == "mean")
for(ii in 1:r)
{
j <- 1
for(jj in 1:c)
{
m3[ii,jj]<-mean(as.numeric(m[i:(i+(f-1)), j:(j+(f-1))]))
j <- j+s
}
i <- i+s
}
